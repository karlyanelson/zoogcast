---
layout: layout_base.njk
img: social-img.png
---

<ul class="space-y-16">
  {%- for post in collections.post | reverse -%}
  <li class="border border-white">
    <img src="/images/{{post.data.img}} " alt="" class="post-list-img" />
    <a class="block text-3xl my-8" href="{{ post.url }}">{{ post.data.title }}</a>
  </li>
  {%- endfor -%}
</ul>
